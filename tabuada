programa {
  funcao inicio() {
    inteiro contador,limite,resultado,tabuada

    contador = 0
    limite = 10

      escreva("Digite a Tabuada que deseja: ")
       leia(tabuada)

    
    faca{
      resultado = tabuada * contador
      escreva( tabuada + " X " + contador + " = " + resultado "\n")
      contador++
    }enquanto (contador<=limite)
  }
}
